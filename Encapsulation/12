		mas[i].Show();
		cout << "\n���������� �� ������ ��������� �� ����� C" << i+1 << " = " << mas[i].distance();
	}
	//������������ ������
	coordinates* p_mas = new coordinates[3];
	for (int i = 0; i < 3; i++) {
		cout << "\n\n����� D" << i + 1;
		p_mas[i].Read();
		cout << "\n����� D" << i + 1;
		p_mas[i].Show();
		cout << "\n���������� �� ������ ��������� �� ����� D" << i + 1 << " = " << p_mas[i].distance();
	}
	delete[] p_mas;
	//����� ������� make_coordinates
