	cout << "\n\n����� F";
	cout << "\nx = "; cin >> x;
	cout << "y = "; cin >> y;
	coordinates F = make_coordinates(x, y);
	cout << "\n����� F";
	F.Show();
	cout << "\n���������� �� ������ ��������� �� ����� F = " << F.distance() << endl;
	system("pause");
}

